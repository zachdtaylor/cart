{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build:css": "npx tailwindcss -o ./app/tailwind.css --minify",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "build": "run-s build:*",
    "codegen": "graphql-codegen --config codegen.ts",
    "deploy": "fly deploy --remote-only",
    "dev:css": "npx tailwindcss -o ./app/tailwind.css --watch",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "dev": "run-p dev:*",
    "start": "cross-env NODE_ENV=production remix-serve build",
    "test:e2e": "INCLUDE_TEST_ROUTES=true npx playwright test",
    "test:e2e:ui": "INCLUDE_TEST_ROUTES=true npx playwright test --ui"
  },
  "dependencies": {
    "@prisma/client": "^3.15.2",
    "@remix-run/node": "^1.19.1",
    "@remix-run/react": "^1.19.1",
    "@remix-run/serve": "^1.19.1",
    "cross-env": "^7.0.3",
    "cryptr": "^6.0.3",
    "form-data": "^4.0.0",
    "graphql": "^16.7.1",
    "graphql-request": "^6.1.0",
    "isbot": "^3.6.10",
    "mailgun.js": "^8.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "uuid": "^9.0.0",
    "zod": "^3.17.10"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.2.16",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@playwright/test": "^1.35.1",
    "@remix-run/dev": "^1.19.1",
    "@remix-run/eslint-config": "^1.19.1",
    "@types/cryptr": "^4.0.1",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/react-modal": "^3.16.0",
    "@types/uuid": "^8.3.4",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.15.2",
    "tailwindcss": "^3.1.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
